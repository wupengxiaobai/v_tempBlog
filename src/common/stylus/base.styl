// @import './variable.styl';
body, html {
    line-height: 1;
    font-family: 'Microsoft Yahei', 'PingFang SC', 'STHeitiSC-Light', 'Helvetica-Light', arial, sans-serif, 'Droid Sans Fallback';
    user-select: none;
    -webkit-tap-highlight-color: transparent;
}

.clear-fix {
    &::after {
        content: '';
        display: table;
        clear: both;
    }
}

.container {
    width: 1200px;
    margin: 0 auto;
}